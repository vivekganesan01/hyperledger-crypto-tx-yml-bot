{
    "name": "my-network-cts",
    "version": "1.0.0",
    "client": {
        "organization": "cts",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "cts": {
            "mspid": "ctsMSP",
            "peers": [
                "peer0.cts.site.com",
                "peer1.cts.site.com",
                "peer2.cts.site.com"
            ],
            "certificateAuthorities": [
                "ca.cts.site.com"
            ]
        }
    },
    "peers": {
        "peer0.cts.site.com": {
            "url": "grpcs://localhost:7051",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/cts.site.com/tlsca/tlsca.cts.site.com-cert.pem"
            },
            "grpcOPtions": {
                "ssl-target-name-override": "peer0.cts.site.com",
                "hostnameOverride": "peer0.cts.site.com"
            }
        },
        "peer1.cts.site.com": {
            "url": "grpcs://localhost:8051",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/cts.site.com/tlsca/tlsca.cts.site.com-cert.pem"
            },
            "grpcOPtions": {
                "ssl-target-name-override": "peer1.cts.site.com",
                "hostnameOverride": "peer1.cts.site.com"
            }
        },
        "peer2.cts.site.com": {
            "url": "grpcs://localhost:9051",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/cts.site.com/tlsca/tlsca.cts.site.com-cert.pem"
            },
            "grpcOPtions": {
                "ssl-target-name-override": "peer2.cts.site.com",
                "hostnameOverride": "peer2.cts.site.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.cts.site.com": {
            "url": "https://localhost:7054",
            "caName": "ca-cts",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/cts.site.com/ca/ca.cts.site.com-cert.pem"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}